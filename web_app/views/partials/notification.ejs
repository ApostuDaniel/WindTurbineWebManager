<div class="notification" id="id<%= notification.id %>">
    <%= notification.buyer.firstName %> <%= notification.buyer.lastName %> would
    like to purchase turbine <%=notification.turbine.name %>
  <div>
    <button onclick="accept(`<%= notification.turbine._id %>`, `<%= notification.buyer._id %>`, `<%= notification.id %>`)">Accept</button>
    <button id="declineButton">Decline</button>
  </div>
</div>
