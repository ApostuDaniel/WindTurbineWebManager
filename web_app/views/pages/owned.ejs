<!DOCTYPE html>
<html lang="ro">
  <% var dir = process.cwd() %> <link rel="icon"
  href=https://webprojectrdv.s3.amazonaws.com/favicon.png>
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <%- include(dir+'/public/css/ownedPageStyle'); %> <%-
    include(dir+'/public/css/navBarStyle'); %> <%-
    include(dir+'/public/css/filtersStyle'); %>
    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <link
      href="https://fonts.googleapis.com/css2?family=Exo+2:wght@100&display=swap"
      rel="stylesheet"
    />
    <script src="https://cdn.jsdelivr.net/npm/chart.js"></script>
    <%- include(dir+'/public/scripts/chartCreation'); %>
    <title>Owned turbines</title>
  </head>
  <body>
    <%- include(dir + "/views/partials/navBar") %> <%- include(dir +
    "/views/partials/filters") %>

    <div class="addTurbine" onclick="location.href='/pages/createTurbine'">
      <img src=https://webprojectrdv.s3.amazonaws.com/addButton.png>
    </div>

    <div class="cardList">
      <% for(turbine of turbines) {%> <%- include(dir +
      "/views/partials/ownedTurbine", {turbine:turbine, chart:chartData[turbine._id + 'chart']}) %> <% } %>
    </div>
    <%- include (dir+'/public/scripts/protectedRouteScript'); %>
    <%- include (dir+'/public/scripts/publicownedPageScript'); %>
  </body>
</html>
