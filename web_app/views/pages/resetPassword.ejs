<!DOCTYPE html>
<html>
  <% var dir = process.cwd() %> <%- include(dir+'/public/css/loginStyle'); %>
  <link rel="icon" href=https://webprojectrdv.s3.amazonaws.com/favicon.png>
  <link rel="preconnect" href="https://fonts.googleapis.com">
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
  <link href="https://fonts.googleapis.com/css2?family=Jura:wght@300&display=swap" rel="stylesheet">
  <head>
    <title>Log in</title>
  </head>

  <body>
    <p id="title1">
      Wind
      <span id="title2">Turbine Management</span>
    </p>
    <form class="login" id="form">
      <label aria-hidden="true">Reset Password</label>
      <input type="email" id="email" name="email" placeholder="Email" />
    
      
      <button id="log" onclick="sendEmail()">Done</button>
      <p id="errormessage"></p>
    </form>

    <%- include (dir+'/public/scripts/resetPassScript'); %> <%- include
    (dir+'/public/scripts/protectedRouteLoginScript'); %>
    <script src="https://smtpjs.com/v3/smtp.js"></script>
  </body>
</html>
